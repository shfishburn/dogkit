{
  "name": "web",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "sync:specs": "node scripts/sync-specs.mjs",
    "gen:ingredients-db": "node ../scripts/generate_ingredients_db.mjs",
    "gen:ingredients": "../.venv/bin/python ../scripts/extract_median_nutrients.py --input ../data/kyokon-synthetic-ingredients.json --format json --output src/data/ingredients_medians.json",
    "predev": "npm run gen:ingredients-db && npm run sync:specs && npm run gen:ingredients",
    "dev": "astro dev",
    "prebuild": "npm run gen:ingredients-db && npm run sync:specs",
    "build": "astro build",
    "preview": "astro preview",
    "lint": "eslint . && astro check",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/sitemap": "^3.7.0",
    "@fontsource-variable/inter": "^5.2.8",
    "@nanostores/persistent": "^1.3.3",
    "astro": "^5.17.1",
    "nanostores": "^1.1.0",
    "remark-gfm": "^4.0.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.6",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "eslint": "^9.39.2",
    "eslint-plugin-astro": "^1.6.0",
    "typescript": "^5.9.3"
  }
}
